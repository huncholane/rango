{
  "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "root",
          "commands": ["source venv/Scripts/activate"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "frontserver",
          "commands": ["./dev front"]
        },
        {
          "name": "backserver",
          "commands": ["./dev back"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "frontend",
          "commands": ["source venv/Scripts/activate", "cd rango/frontend"]
        },
        {
          "name": "backend",
          "commands": ["source venv/Scripts/activate", "cd rango/backend"]
        }
      ]
    }
  ]
}
